<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <Summary>Example Razor Pages app showing how to use Sorling.SqlConnAuthWeb for SQL authentication.</Summary>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\SQLConnAuthWeb\SqlConnAuthWeb.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Content Update="appsettings.json">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
  </ItemGroup>

</Project>
