{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "SqlAuthOptions": {
    "AllowIntegratedSecurity": false,
    "AllowTrustServerCertificate": true,
    "AllowLoopbackConnections": true,
    "AllowPrivateNetworkConnections": true,
    "AllowedIPAddresses": [
      "127.0.0.1/32",
      "192.168.1.0/24"
    ],
    "ExcludeDatabaseFilter": [
      "mAst*"
    ],
    "IncludeDatabaseFilter": [
      "*"
    ]
  },
  "SqlAuthUIOptions": {
    "ThemeSwitcherLocalStorageName": "sqlauththeme",
    "LightBackgroundColor": "#fff",
    "LightForegroundColor": "#222",
    "DarkBackgroundColor": "#0F0F0F",
    "DarkForegroundColor": "#f5f5f5",
    "MaxRecentItems":  20
  }
}
